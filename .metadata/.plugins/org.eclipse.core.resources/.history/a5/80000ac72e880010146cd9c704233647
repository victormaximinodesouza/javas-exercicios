	public static void main(String[] args) {
		List<String> cores = new ArrayList<>();
		
		cores.add("Vermelho");
		cores.add("Azul");
		cores.add("Verde");
		
		cores.add(2, "Amarelo");
		
		for(String f: cores) {
			System.out.println(f);
			
	}
