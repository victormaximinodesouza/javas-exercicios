package att_listas;

import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;



public class listas_02 {

	public static void main(String[] args) {
		List<String> node = new ArrayList<>();
		
		
		node.add("Maça");
		node.add("Uva");
		node.add("Laranja");
		
		for(String f : node) {
			System.out.print(f + " ");
		}
		
		System.out.println("\n -----------");
		for(String o : node) {
			node.remove(1 , "Maça");
			System.out.println(o);
		}
		
		

	}
}
